{
  "name": "dashipopashi",
  "version": "0.0.3",
  "description": "An experimental library for integrating interactive charts into existing JavaScript applications.",
  "type": "module",
  "files": ["dist", "README.md"],
  "keywords": [
    "typescript",
    "library",
    "framework",
    "dashboard",
    "plotting",
    "charting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bcdev/dashi.git"
  },
  "bugs": {
    "url": "https://github.com/bcdev/dashi/issues"
  },
  "homepage": "https://github.com/bcdev/dashi/blob/main/dashi/README.md",
  "author": "Brockmann Consult GmbH",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "module": "./dist/dashi.js",
  "main": "./dist/dashi.umd.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/dashi.js",
      "require": "./dist/dashi.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.5",
    "@mui/material": "^5.15",
    "microdiff": "^1.4.0",
    "plotly.js-dist-min": "^2.35.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-plotly.js": "^2.6.0",
    "zustand": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/plotly.js": "^2.33.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-plotly.js": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "glob": "^11.0.0",
    "jsdom": "^25.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vite-plugin-dts": "^4.2.4",
    "vitest": "^2.1.1"
  }
}
